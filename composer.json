{
  "name": "src-run/augustus-polyfill-library",
  "license": "MIT",
  "type": "library",
  "homepage": "https://src.run/augustus-polyfill-library",
  "description": "Symfony/polyfill-* and provides an Error/Throwable implementation for pre-7.0 PHP release.",
    "support" : {
    "email": "rmf@src.run",
    "issues": "https://src.run/augustus-polyfill-library/issues",
    "source": "https://src.run/augustus-polyfill-library/git"
  },
  "keywords" : [ "augustus-polyfill-library", "augustus", "polyfill", "library", "rmf", "src-run" ],
  "authors" : [
    {
      "name": "Rob Frawley 2nd",
      "email": "rmf@src.run",
      "homepage": "https://src.run/rmf",
      "role": "Project Lead"
    }
  ],
  "require" : {
    "php": "~7.0",
    "paragonie/random_compat": "~2.0",
    "symfony/polyfill": "~1.2"
  },
  "require-dev" : {
    "codacy/coverage": "~1.0",
    "friendsofphp/php-cs-fixer": "~2.0",
    "phpunit/phpunit": "~5.4",
    "satooshi/php-coveralls": "~1.0",
    "sllh/php-cs-fixer-styleci-bridge": "~2.1"
  },
  "autoload" :     {
    "psr-4" : {
      "SR\\Polyfill\\": "lib/"
    },
    "files" : [
      "bootstrap.php"
    ]
  },
  "autoload-dev" : {
    "psr-4" : {
      "SR\\Polyfill\\Tests\\": "tests/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "bin-dir": "bin",
    "sort-packages": true
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.7-dev"
    }
  }
}
