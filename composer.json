{
  "name"        : "src-run/augustus-polyfill-library",
  "license"     : "MIT",
  "type"        : "library",
  "homepage"    : "https://src.run/augustus-polyfill-library",
  "description" : "library that includes symfony/polyfill-* and provides an Error/Throwable implementation for pre-7.0 PHP release",
    "support" : {
    "email"  : "rmf@src.run",
    "issues" : "https://src.run/augustus-polyfill-library/issues",
    "source" : "https://src.run/augustus-polyfill-library/git"
  },
  "keywords" : [
    "augustus-polyfill-library", "augustus", "polyfill", "library", "rmf", "src-run"
  ],
  "authors" : [
    {
      "name"    : "Rob Frawley 2nd",
      "email"   : "rmf@src.run",
      "homepage": "https://src.run/rmf",
      "role"    : "Project Lead"
    }
  ],
  "require" : {
    "php"                       : "^5.6||^7.0",
    "paragonie/random_compat"   : "^1.4||^2.0",
    "ircmaxell/password-compat" : "^1.0",
    "symfony/polyfill"          : "^1.1"
  },
  "require-dev" : {
    "phpunit/phpunit" :        "^5.0",
    "codacy/coverage" :        "^1.0",
    "satooshi/php-coveralls" : "^1.0"
  },
  "autoload" :     {
    "psr-4" : {
      "SR\\Polyfill\\" : "lib/"
    },
    "files" : [
      "bootstrap.php"
    ]
  },
  "autoload-dev" : {
    "psr-4" : {
      "SR\\Polyfill\\Tests\\" : "tests/"
    }
  },
  "minimum-stability" : "dev",
  "prefer-stable"     : true,
  "config": {
    "bin-dir" : "bin"
  },
  "extra": {
    "branch-alias": {
      "dev-master" : "0.6-dev"
    }
  }
}
